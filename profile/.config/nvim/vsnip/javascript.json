{
    "shebang": {
        "prefix": [
            "#!",
            "shebang"
        ],
        "body": "#!/usr/bin/env node"
    },
    "process.exit()": {
        "prefix": "process.exit()",
        "body": "process.exit()"
    },
    "console.log( $1 )": {
        "prefix": "console.log",
        "body": "console.log( $1 )"
    },
    "JSON.stringify": {
        "prefix": "json.stringify( $1 )",
        "body": "JSON.stringify( $1 )"
    },
    "JSON.parse": {
        "prefix": "json.parse( $1 )",
        "body": "JSON.parse( $1 )"
    },
    "import $1 from \"$2\";": {
        "prefix": "import",
        "body": "import $1 from \"$2\";"
    },
    "#core/$1": {
        "prefix": "#core",
        "body": "#core/$1"
    },
    "eslint-disable-next-line": {
        "prefix": "eslint-disable-next-line",
        "body": "// eslint-disable-next-line $1"
    },
    "eslint-disable-line": {
        "prefix": "eslint-disable-line ",
        "body": "// eslint-disable-line $1"
    },
    "eslint-disable": {
        "prefix": "eslint-disable",
        "body": "/* eslint-disable $1 */"
    },
    "for": {
        "prefix": "for ",
        "body": "for ( let n = 0; n < 1; n++ ) {\n    $1\n}"
    },
    "function": {
        "prefix": "function ",
        "body": "function $1 () {\n    $2\n}"
    },
    "if-else": {
        "prefix": "if ",
        "body": "if ( $1 ) {\n    $2\n}\nelse if ( $3 ) {\n    $4\n}\nelse {\n    $5\n}"
    }
}
